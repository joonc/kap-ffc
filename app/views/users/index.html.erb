<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <h1> Users Admin Page </h1>
      <table class="table table-striped">
	<thead>
          <tr>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Signup Date</th>
          </tr>
	</thead>
	<tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= user.email %></td>
            <td><%= user.created_at.localtime.strftime("%b %-d, %Y") %></td>
          </tr>
          <% end %>
	</tbody>
      </table>
    </div>
  </div>
</div>
